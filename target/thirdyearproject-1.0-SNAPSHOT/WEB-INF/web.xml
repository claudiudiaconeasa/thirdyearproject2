<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>JohnRylands</servlet-name>
        <servlet-class>JohnRylands</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>JohnRylands</servlet-name>
        <url-pattern>/JohnRylands</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WhitworthHall</servlet-name>
        <servlet-class>WhitworthHall</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WhitworthHall</servlet-name>
        <url-pattern>/WhitworthHall</url-pattern>
    </servlet-mapping>


</web-app>