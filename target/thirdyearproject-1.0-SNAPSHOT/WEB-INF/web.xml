<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>welcome.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>JohnRylands</servlet-name>
        <servlet-class>JohnRylands</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>JohnRylands</servlet-name>
        <url-pattern>/JohnRylands</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>About</servlet-name>
        <servlet-class>About</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>About</servlet-name>
        <url-pattern>/About</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EventSelector</servlet-name>
        <servlet-class>EventSelector</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EventSelector</servlet-name>
        <url-pattern>/EventSelector</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EventSelectorComputing</servlet-name>
        <servlet-class>EventSelectorComputing</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EventSelectorComputing</servlet-name>
        <url-pattern>/EventSelectorComputing</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Uploader</servlet-name>
        <servlet-class>Uploader</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Uploader</servlet-name>
        <url-pattern>/Uploader</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Downloader</servlet-name>
        <servlet-class>Downloader</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Downloader</servlet-name>
        <url-pattern>/Downloader</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WhitworthHall</servlet-name>
        <servlet-class>WhitworthHall</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WhitworthHall</servlet-name>
        <url-pattern>/WhitworthHall</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>JsonFiles</servlet-name>
    <servlet-class>JsonFiles</servlet-class>
</servlet>

    <servlet-mapping>
        <servlet-name>JsonFiles</servlet-name>
        <url-pattern>/json-files/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Preview</servlet-name>
        <servlet-class>Preview</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Preview</servlet-name>
        <url-pattern>/preview/*</url-pattern>
    </servlet-mapping>

</web-app>